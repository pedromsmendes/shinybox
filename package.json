{
  "name": "shinybox",
  "version": "0.1.0",
  "author": "pmsmendes",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.development next dev",
    "build": "env-cmd -f .env.production next build",
    "start": "next start",
    "lint": "next lint",
    "gqlgen:fetch": "env-cmd -f .env.development esm-ts-node src/tools/fetchSchema",
    "gqlgen:generate": "graphql-codegen --config gqlgen.yml",
    "gqlgen": "npm run gqlgen:fetch && npm run gqlgen:generate"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@fontsource/roboto": "^4.5.3",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/named-operations-object": "^2.2.1",
    "@graphql-codegen/near-operation-file-preset": "^2.2.6",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-codegen/typescript-operations": "^2.3.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.8",
    "@mantine/core": "^4.0.0",
    "@mantine/dates": "^4.0.0",
    "@mantine/dropzone": "^4.0.0",
    "@mantine/hooks": "^4.0.0",
    "@mantine/modals": "^4.0.0",
    "@mantine/next": "^4.0.0",
    "@mantine/notifications": "^4.0.0",
    "@mantine/spotlight": "^4.0.0",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.8",
    "env-cmd": "^10.1.0",
    "graphql": "^16.3.0",
    "next": "12.1.0",
    "node-fetch": "^2.6.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tabler-icons-react": "^1.37.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "@types/node-fetch": "^2.6.1",
    "@types/react": "17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-next": "12.1.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "esm": "^3.2.25",
    "esm-ts-node": "^1.0.7",
    "typescript": "4.5.5"
  }
